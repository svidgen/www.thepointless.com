{
  "name": "thepointless.com",
  "version": "1.0.0",
  "description": "source for thepointless.com",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && yarn ex-gratia && webpack",
    "build:watch": "rimraf dist && webpack --mode development --progress --watch",
    "serve": "http-server ./dist -p 9999 -o",
    "start": "concurrently -k -p \"[{name}]\" -n \"Build,Serve\" -c \"cyan.bold,green.bold\" \"npm run build:watch\" \"npm run serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svidgen/www.thepointless.com.git"
  },
  "author": "Jon Wire",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/svidgen/www.thepointless.com/issues"
  },
  "homepage": "https://github.com/svidgen/www.thepointless.com#readme",
  "devDependencies": {
    "concurrently": "^6.0.2",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "highlight.js": "^11.1.0",
    "http-server": "^13.0.2",
    "jsdom": "^16.5.2",
    "marked": "^2.0.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "aws-amplify": "^4.3.2",
    "ex-gratia": "^1.0.3",
    "wirejs-dom": "^1.0.3"
  }
}
